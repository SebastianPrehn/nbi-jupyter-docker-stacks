   "source": [
    "from iminuit import Minuit\n",
    "\n",
    "\n",
    "def f(x, y, z):\n",
    "    return (x - 2) ** 2 + (y - 3) ** 2 + (z - 4) ** 2\n",
    "\n",
    "\n",
    "m = Minuit(f)\n",
    "\n",
    "m.migrad()  # run optimiser\n",
    "print(m.values)  # {'x': 2,'y': 3,'z': 4}\n",
    "\n",
    "m.hesse()   # run covariance estimator\n",
    "print(m.errors)  # {'x': 1,'y': 1,'z': 1}"
   ]
  }
 ],
